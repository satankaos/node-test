<%- include("template/cabecera", {tituloWeb: 'Nuevo entrenador' }) %>
    <div class="container">
        <h1>Crear nuevo Pokemon</h1>
        <form action="/entrenador" class="my-2" method="POST">
            <input type="text" placeholder="Nombre entrenador" class="form-control my-2" name="Nombre">
            <input type="text" placeholder="Bio" class="form-control my-2" name="Bio">
            <input type="text" placeholder="ciudad" class="form-control my-2" name="Ciudad">
            <input type="text" placeholder="lista" class="form-control my-2" name="Lista_Pokemon">
            <button type="submit" class="btn btn-dark btn-block">
                Agregar
            </button>
        </form>
    </div>
<%- include("template/footer") %>
